<h1>Stocks</h1>

<table>
  <tr>
    <th>Symbol</th>
    <th>QTY</th>
    <th>User</th>
    <th></th>
  </tr>

  <% @stocks.each do |stock| %>
    <tr>
      <td><%= stock.symbol %></td>
      <td><%= stock.quantity %></td>
      <td><%= stock.user_id %></td>
      <td><%= link_to 'More Info', stock_path(stock) %></td>
    </tr>
  <% end %>
</table>
